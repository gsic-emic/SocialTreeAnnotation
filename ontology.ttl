#Me invento el namespace para la ontología propia que voy a crear
@prefix ex: <http://example.com/ontology/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix ifn: <http://crossforest.eu/ifn/ontology/> .
@prefix vann: <http://purl.org/vocab/vann/> .


#Defino la ontología propia como rdf:type Ontology. Es decir, es miembro de la clase de owl Ontología
ex: a owl:Ontology ;
    rdfs:label "Social forest app Ontology"@en , "Ontología de la aplicación forestal social"@es ;
    rdfs:comment "Ontology to publish data of the Social Forest inventory"@en , "Ontología para publicar datos del Inventario Forestal Social" ;
    owl:versionInfo 0.1 ;
    dc:creator <https://www.gsic.uva.es/members/cmayo> , <https://www.gsic.uva.es/members/jimena> ; #Etc...
    dc:date "2019-10-12"^^xsd:date ;
    owl:imports ex: , ifn: ; #Si hubiera que importar ontologías...
    vann:preferredNamespacePrefix "ex" . #The preferred namespace prefix to use when using terms from this vocabulary in an XML document.

#################################################################
#    Classes
#################################################################

#Defino una clase árbol propia que ¿es subclase de ifn:Tree?
ex:Tree a owl:Class , rdfs:Class ;
  rdfs:subClassOf ifn:Tree ;
  rdfs:label "Tree"@en , "Árbol"@es ;
  rdfs:comment "Woody perennial plant with an elongated stem supporting branches"@en , "Planta vivaz (que vive más de dos años, de tallo leñoso, que se ramifica a cierta altura del suelo"@es ; 
  rdfs:isDefinedBy ex: . #no sé si es necesario.

ex:TreePart a owl:Class , rdfs:Class ;
  rdfs:label "Tree part"@en , "Parte del árbol"@es ;
  rdfs:comment "Part of a tree that it is shown in the image"@en , "Parte del árbol que se muestra en la imagen"@es ;
  rdfs:isDefinedBy ex: . #no sé si es necesario.

ex:Leave a owl:Class , rdfs:Class ;
  rdfs:subClassOf ex:TreePart ;
  rdfs:label "Leave"@en , "Hoja"@es ;
  rdfs:comment "Leave"@en , "Hoja de árbol"@es ;
  rdfs:isDefinedBy ex: .

ex:Fruit a owl:Class , rdfs:Class ;
  rdfs:subClassOf ex:TreePart ;
  rdfs:label "Fruit"@en , "Fruto"@es ;
  rdfs:comment "Fruit which grows on a tree"@en , "Fruto que crece en un árbol"@es ;
  rdfs:isDefinedBy ex: .

ex:Trunk a owl:Class , rdfs:Class ;
  rdfs:subClassOf ex:TreePart ;
  rdfs:label "Trunk"@en , "Tronco"@es ;
  rdfs:comment ""@en , ""@es ;
  rdfs:isDefinedBy ex: .

ex:Branch a owl:Class , rdfs:Class ;
  rdfs:subClassOf ex:TreePart ;
  rdfs:label "Branch"@en , "Rama"@es ;
  rdfs:comment ""@en , ""@es ;
  rdfs:isDefinedBy ex: .

ex:Crown a owl:Class , rdfs:Class ;
  rdfs:subClassOf ex:TreePart ;
  rdfs:label "Crown"@en , "Copa"@es ;
  rdfs:comment ""@en , ""@es ;
  rdfs:isDefinedBy ex: .

ex:GeneralView a owl:Class , rdfs:Class ;
  rdfs:subClassOf ex:TreePart ;
  rdfs:label "General"@en , "General"@es ;
  rdfs:comment "General view"@en , "Vista general"@es ;
  rdfs:isDefinedBy ex: .

#################################################################
#    Object Properties
#################################################################
ex:hasPart a owl:ObjectProperty , rdf:Property ;
  rdfs:label "has part"@en , "tiene parte"@es ;
  rdfs:comment "part of the tree"@en , "parte del árbol"@es ;
  rdfs:domain ex:Tree ;
  rdfs:range ex:TreePart ;
  rdfs:isDefinedBy ex: .

